## Disable monitoring of logstash to remove local elastic dependency
XPACK_MONITORING_ENABLED=false

##Environment variables generated by ansible
pipeline_name=main
LOG_LEVEL=info
#Kafka Input Settings
kafka_input_bootstrap_servers=localhost:9092
kafka_input_topic=stardust_flow
kafka_input_client_id=logstash_1
kafka_input_group_id=stardust_flow_prod
kafka_input_consumer_threads=3
kafka_input_max_poll_records=37000
kafka_input_max_poll_interval_ms=300000
kafka_input_auto_offset_reset=latest
kafka_input_ssl_keystore_location=/etc/stardust/pipeline/certificates/kafka_user.p12
kafka_input_ssl_key_password=CHANGEME
kafka_input_ssl_keystore_password=CHANGEME
kafka_input_ssl_truststore_location=/etc/stardust/pipeline/certificates/kafka_ca.p12
kafka_input_ssl_truststore_password=CHANGEME
##Elastic Output Settings
elastic_output_host_url=https://CHANGEME:9200
elastic_output_user=CHANGEME
elastic_output_password=CHANGEME